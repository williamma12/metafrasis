{
  "tesseract": {
    "base": {
      "url": "https://github.com/tesseract-ocr/tessdata_best/raw/main/grc.traineddata",
      "type": "traineddata",
      "description": "Ancient Greek trained data for Tesseract (best quality)"
    }
  },
  "trocr": {
    "base": {
      "url": "microsoft/trocr-base-handwritten",
      "type": "huggingface",
      "description": "Base trOCR model for handwritten text recognition"
    },
    "large": {
      "url": "microsoft/trocr-large-handwritten",
      "type": "huggingface",
      "description": "Large trOCR model for handwritten text (better quality, slower)"
    }
  },
  "craft": {
    "base": {
      "url": "https://drive.google.com/uc?id=1Jk4eGD7crsqCCg9C9VjCLkMN3ze8kutZ",
      "type": "gdrive",
      "filename": "craft_mlt_25k.pth",
      "description": "CRAFT detector pretrained on MLT (Multi-Lingual Text) dataset"
    },
    "icdar": {
      "url": "https://drive.google.com/uc?id=1bupFXqT-VU6Jjeul13XP7yx2Sg5IHr4J",
      "type": "gdrive",
      "filename": "craft_ic15_20k.pth",
      "description": "CRAFT detector pretrained on ICDAR 2015 dataset"
    }
  },
  "crnn": {
    "base": {
      "url": "https://github.com/clovaai/deep-text-recognition-benchmark/releases/download/v1.0/None-VGG-BiLSTM-CTC.pth",
      "type": "direct",
      "filename": "crnn_vgg_bilstm_ctc.pth",
      "description": "CRNN recognizer with VGG backbone and BiLSTM (English characters + digits)"
    }
  },
  "db": {
    "base": {
      "url": "https://paddleocr.bj.bcebos.com/dygraph_v2.0/en/det_r50_vd_db_v2.0_train.tar",
      "type": "archive",
      "filename": "db_resnet50.pth",
      "description": "DB detector with ResNet50 backbone (multilingual, general purpose)"
    },
    "mobilenet": {
      "url": "https://paddleocr.bj.bcebos.com/dygraph_v2.0/en/det_mv3_db_v2.0_train.tar",
      "type": "archive",
      "filename": "db_mobilenet.pth",
      "description": "DB detector with MobileNetV3 backbone (faster, mobile-friendly)"
    }
  },
  "kraken": {
    "greek": {
      "url": "https://zenodo.org/record/2577813/files/porson.mlmodel",
      "type": "direct",
      "filename": "kraken_greek_porson.mlmodel",
      "description": "Kraken model for Ancient Greek (Porson typeface)"
    },
    "greek_medieval": {
      "url": "https://zenodo.org/record/6542744/files/medieval_greek.mlmodel",
      "type": "direct",
      "filename": "kraken_medieval_greek.mlmodel",
      "description": "Kraken model for Medieval Greek manuscripts"
    },
    "polytonic": {
      "url": "https://zenodo.org/record/6657809/files/polytonic_greek.mlmodel",
      "type": "direct",
      "filename": "kraken_polytonic_greek.mlmodel",
      "description": "Kraken model for Polytonic Greek (with diacritics)"
    }
  }
}
